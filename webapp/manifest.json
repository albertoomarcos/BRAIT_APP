{
	"_version": "1.12.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "miaplicacion.myapp",
		"type": "application",
		"title": "{{appTitle}}",
		"i18n": "i18n/i18n.properties",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "brait_app.view.App",
			"type": "XML",
			"async": false,
			"id": "start"
		},
		"autoPrefixId": true,
		"dependencies": {
			"minUI5Version": "1.34",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.34.0"
				},
				"sap.m": {
					"minVersion": "1.34.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.34.0"
				},"sap.f": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"helloPanel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/HelloPanel.json"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "brait_app.i18n.i18n"
				}
			},
			"address": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/Address.json"
			},
			"prueba": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/facturas.json"
			},
			"jsonWEB": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "https://brait.cc/scpdev/facturas.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "brait_app.view",
				"controlId": "invoices",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
			{
				"pattern": "",
				"name": "overview",
				"target": "overview"
			}, 
			{
				"pattern": "users",
				"name": "users",
				"target": "users"
			}, 
			{
				"pattern": "detail/{pruebaPath}",
				"name": "detail",
				"target": "detail"
			},
			{
				"pattern": ":layout:",
				"name": "master",
				"target": [
					"master",
					"fioridetail"
				]
			},
			{
				"pattern": "fioridetail/{product}/{layout}",
				"name": "fioridetail",
				"target": [
					"master",
					"fioridetail"
				]
			}
			],
			"targets": 
			{
				"overview": {
					"viewID": "overview",
					"viewName": "Overview"
				},
				"detail": {
					"viewId": "detail",
					"viewName": "Detail"
				},
				"users": {
					"viewName": "UsersSQL",
					"viewType": "JS"
				},
				"master": {
					"viewName": "Fiori_Master",
					"controlAggregation": "beginColumnPages"
				},
				"fioridetail": {
					"viewName": "Fiori_Detail",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}